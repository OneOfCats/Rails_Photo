$('.like_panel_<%= @queue_image.id %>').replaceWith('<%= escape_javascript(render  partial: 'like', :locals => {item: @queue_image}) %>');
$('.like-block[data-item-id="<%= @queue_image.id %>"').html('<%= @queue_image.likes_count %>').addClass('active');
$('.like-link[data-item-id="<%= @queue_image.id %>"').attr('href', '/queue_images/<%= @queue_image.id %>/unlike').html('Unlike');
$(".credits").html("<%= current_client.credits %>");